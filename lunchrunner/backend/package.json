{
  "name": "lunchrunner-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "dev": "NODE_ENV=development nodemon --watch src --ext js,ts --exec node src/server.js",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.js\"",
    "migrate": "node src/db/runMigrations.js",
    "seed": "node src/db/seedDaten.js",
    "test": "vitest run"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "zod": "^3.23.8",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.4.2",
    "vitest": "^1.6.0",
    "pg-mem": "^2.1.3"
  }
}
